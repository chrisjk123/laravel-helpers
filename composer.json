{
    "name": "chrisjk123/laravel-helpers",
    "description": "A helpers extension of Laravel's helper methods",
    "keywords": [
        "laravelhelpers",
        "laravel-helpers"
    ],
    "homepage": "https://github.com/chrisjk123/laravel-helpers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Christopher Kelker",
            "email": "christopherjk123@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3|^8.0|^8.1",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
          "Chriscreates\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Chriscreates\\Helpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    }
}
